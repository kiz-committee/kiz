object Animal
    name = ""
    fn __call__(this, n)
        o = create(this)
        o.name = n

        return o
    end

    fn get_name(this)
        return this.name
    end
end

object Dog: Animal
    fn bark(this)
        print(this.name, "is barking")
    end
end

dog = Dog("Tim")
print(dog.get_name())
dog.bark()